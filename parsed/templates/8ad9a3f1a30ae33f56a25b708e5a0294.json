  "components/common/footer": "{{#if banners.bottom}}\n    {{#if settings.data_tag_enabled}}\n        <div class=\"banners\" data-banner-location=\"bottom\">\n            {{#each (limit banners.bottom_metadata 1)}}\n                <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-name=\"{{this.banner-name}}\">\n                    <div data-event-type=\"promotion-click\">\n                        {{{this.content}}}\n                    </div>\n                </div>\n            {{/each}}\n        </div>\n    {{else}}\n        <div class=\"banners\" data-banner-location=\"bottom\">\n            <div class=\"banner\">\n                {{{limit banners.bottom 1}}}\n            </div>\n        </div>\n    {{/if}}\n\n{{/if}}\n{{{region name=\"home_above_footer\"}}}\n<footer class=\"footer\" role=\"contentinfo\">\n    <h2 class=\"footer-title-sr-only\">{{lang 'footer.title'}}</h2>\n    {{#if theme_settings.theme_fashion}}\n    <div class=\"newslatter-footer\">\n        <div class=\"container\">\n            <div class=\"row\">\n                <div class=\"col-12\">\n                    <div class=\"webi-subscribe\">\n                        {{> components/common/subscription-form}}\n                    </div>      \n                </div>\n            </div>\n        </div>\n    </div>\n    <!-- footer second -->\n    <div class=\"footer-nav\">\n        <div class=\"container\">\n            <div class=\"row\">\n                <!-- about us section -->\n                <div class=\"col-lg-3 col-sm-3 col-12 foot-about fborder\">\n                    <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"storeInfo\">\n                    <h3 class=\"footer-info-heading titleWrapper inactive\">{{lang 'footer.about'}}\n                        <div class=\"collapse-icon\">\n                            <div class=\"acc-close\"></div>\n         