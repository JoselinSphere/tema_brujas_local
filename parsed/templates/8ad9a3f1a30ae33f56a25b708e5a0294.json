{moment format=\"YYYY\"}} {{settings.store_name}} </p>\n                        </div>\n                    {{/if}}\n                </div>\n                <div class=\"col-md-6 col-12 text-right\">\n                    {{> components/common/payment-icons}}\n                </div>\n            </div>\n        </div>\n    </div>\n    <a href=\"\" id=\"scroll\" title=\"Scroll to Top\" style=\"display: block;\">\n        <svg width=\"16px\" height=\"16px\"><use xlink:href=\"#arrow-right\"></use></svg>\n    </a>\n    {{{region name=\"ssl_site_seal--global\"}}}\n</footer>\n",
  "components/common/subscription-form": "<h3 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</h3>\n<p>{{lang 'newsletter.subscribe_intro'}}</p>\n\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\"\n                       id=\"nl_email\"\n                       name=\"nl_email\"\n                       type=\"email\"\n                       value=\"{{customer.email}}\"\n                       placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\n                       aria-describedby=\"alertBox-message-text\"\n                       aria-required=\"true\"\n                       required\n                >\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\"\n                       type=\"submit\"\n                       value=\"{{lang 'newsletter.subscribe_submit'}}\"\n                >\n            </div>\n            {{#if settings.show_newsletter_summary }}\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\n            {{/if}}\n        </div>\n    </fieldset>\n</form>\n",
  "components/common/social-links": "{{#if social_media}}\n<span class=\"social-share-icon-heading\">{{lang 'social.connect'}}</span>\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\"\n                   href=\"{{url}}\"\n                   target=\"_blank\"\n                   rel=\"noopener\"\n                   title=\"{{{capitalize display_name}}}\"\n                >\n                    <span class=\"aria-description--hidden\">{{{capitalize display_name}}}</span>\n                    <svg>\n                        <use xlink:href=\"#icon-{{display_name}}\"/>\n                    </svg>\n                </a>\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n",
  "components/common/payment-icons": "{{#with theme_settings}}\n    {{#or show_accept_amex show_accept_discover show_accept_mastercard show_accept_paypal show_accept_visa show_accept_amazonpay show_accept_googlepay show_accept_klarna}}\n    <div class=\"footer-payment-icons\">\n        {{#if show_accept_amex}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-american-express\"></use></svg>\n        {{/if}}\n        {{#if show_accept_discover}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-discover\"></use></svg>\n        {{/if}}\n        {{#if show_accept_mastercard}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-mastercard\"></use></svg>\n        {{/if}}\n        {{#if show_accept_paypal}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-paypal\"></use></svg>\n        {{/if}}\n        {{#if show_accept_visa}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-visa\"></use></svg>\n        {{/if}}\n        {{#if show_accept_amazonpay}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-amazonpay\"></use></svg>\n        {{/if}}\n        {{#if show_accept_googlepay}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-googlepay\"></use></svg>\n        {{/if}}\n        {{#if show_accept_klarna}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-klarna\"></use></svg>\n        {{/if}}\n    </div>\n    {{/or}}\n{{/with}}\n"
